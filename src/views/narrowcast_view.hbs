<main>
        <section class="current--event">
                <div class="event--header">
                    <div class="comming--soon-timer">
                        <div class="hours">
                            <span>HOURS</span>
                            <span>02</span>
                        </div>
                        <div class="minutes">
                            <span>MINUTES</span>
                            <span>30</span>
                        </div>
                        <div class="seconds">
                            <span>SECONDS</span>
                            <span>06</span>
                        </div>
                        <span>COMMING SOON</span>
                    </div>
                    <div class="logo">
                        <img src="img/logo.png" alt="" srcset="">
                    </div>
                </div>
                <div class="event--details">
                    <h1>Lorem Ipsum dolor sit amet,contsection adiption elit</h1>
                    <p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s</p>
                   <div class="icon--events">


                    <div class="event--speaker">
                        <img src="img/employee.png" alt="speaker icon">
                        <span>SPEAKER</span>
                        <h3>Mostafa Dahduoh</h3>
                    </div>
                    <div class="event--time">
                        <img src="img/stopwatch.png" alt="stopwatch icon">
                        <span>DURATION</span>
                        <h3>sss - 3:30PM</h3>
                    </div>
                </div>
                </div>
            </section>
            <section class="upcoming--events">
                <h2>Upcoming Events</h2>
                {{#each response.data}}
                <div class="">
                  <div class="event">
                    <div class="event--day">
                      <span>time</span>
                      <span>10</span>
                    </div>
                    <div class="event--tittle">
                      <span>{{this.startDate}}</span>
                      <h3>{{this.title}}</h3>
                    </div>
                </div>
                    {{/each}}
            </section>
</main>
<link rel="stylesheet" href="/css/style.css">
