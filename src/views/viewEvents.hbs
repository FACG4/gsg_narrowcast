<!DOCTYPE html>
<html>
<body>
  <h1> Hello there</h1>
<div class="container">
<div class="boxs">
  {{#each result}}
<div class="box">
  <div class="innerbox">
    <a class="a" href="/details/{{this.id}}"></a>
  </div>
  <div class="innerboxinfo">
    <div>
    <p class="info">{{this.id}} NIS </p>
  </div>
  <div>
  <p class="info">{{this.hall}} NIS </p>
</div>
  </div>

  <div class="innerboxinfo">
    <div>
    <p class="info">{{this.title}} NIS </p>
  </div>
  <div>
  <p class="info">{{this.hall}} NIS </p>
</div>
  </div>

  <div class="innerbox">
    <button type="button" name="button">Edit</button>
    <button type="button" name="button">View</button>
    <button type="button" name="button">Delete</button>

  </div>

  <div id="myModal" class="modal">
    <div class="modal-content">
      <span id="close" class="close">&times;</span>
      <p id="text"></p>
    </div>
  </div>
</div>
  {{/each}}
</div>
</div>

<!-- <script src="/js/saved.js"></script> -->
</body>

</html>
