<!DOCTYPE html>
<html>
<body>
  <h1> {{this.title}} </h1>
<div class="container">
<div class="boxs">
  {{#each result}}
<div class="box">
  <div class="innerbox">
    <a class="a" href="{{this.startDate}}">{{this.startDate}}</a>
  </div>
  <div class="innerboxinfo">
    <div>
    <p class="info">{{this.title}}</p>
  </div>
  <div>
  <p class="info">{{this.speaker}} </p>
</div>
  </div>

  <div class="innerboxinfo">
    <div>
    <p class="info">{{this.hall}} </p>
  </div>
  <div>
  <p class="info">{{this.startDate}}  </p>
</div>
  </div>

  <div class="innerbox">

    <button type="button" name="button"class="editBtn"><a href="/update-event/{{this.id}}">Edit</a></button>

    <button type="button" name="button"class="viewBtn"><a href="/view/{{this.id}}">View</a></button>

    <button type="button" name="button" id="{{this.id}}" class="deleteBtn">Delete</button>

  </div>
</div>
  {{/each}}
  <button type="button" name="button"class="addBtn"><a href="/events/create">Add</a></button>
</div>
</div>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="/js/dom_functions.js"></script>
<script src="/js/view_event_page.js"></script>
</body>

</html>
